{
  "name": "canboatjs",
  "version": "0.0.12",
  "description": "Native javascript version of canboat",
  "main": "index.js",
  "scripts": {
    "test": "mocha --exit",
    "changelog": "github-changes -o sbender9 -r canboatjs -a --only-pulls --use-commit-body --data=pulls  --tag-name=v$npm_package_version",
    "release": "git tag -d v$npm_package_version; npm run changelog && git add CHANGELOG.md && git commit -m 'chore: update changelog' && git tag v$npm_package_version && git push --tags && git push"
  },
  "bin": {
    "analyzerjs": "./bin/analyzerjs",
    "to-pgn": "./bin/to-pgn",
    "candumpanalyzerjs": "./bin/candumpanalyzerjs"
  },
  "keywords": [
    "nmea2000",
    "signalk",
    "signal",
    "k",
    "parser"
  ],
  "author": "Scott Bender <scott@scottbender.net>",
  "contributors": [
    {
      "name": "Kees Verruijt",
      "email": "kees@verruijt.net"
    },
    {
      "name": "Teppo Kurki",
      "email": "teppo.kurki@iki.fi"
    },
    {
      "name": "Jouni Hartikainen",
      "email": "jouni.hartikainen@iki.fi"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "bit-buffer": "^0.2.3",
    "debug": "^3.1.0",
    "int64-buffer": "^0.1.10",
    "lodash": "^4.17.4",
    "serialport": "^6.0.0",
    "split": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-json-equal": "0.0.1",
    "chai-things": "^0.2.0",
    "github-changes": "^1.1.2",
    "mocha": "^5.0.0"
  },
  "optionalDependencies": {
    "socketcan": "^2.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canboat/canboatjs.git"
  }
}
